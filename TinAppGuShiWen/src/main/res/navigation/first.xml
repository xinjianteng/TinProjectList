<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/first"
    app:startDestination="@id/mainActivity">

    <activity
        android:id="@+id/mainActivity"
        android:name="com.tin.projectlist.app.model.gushiwen.MainActivity"
        android:label="MainActivity" />
    <activity
        android:id="@+id/listShowActivity"
        android:name="com.tin.projectlist.app.model.gushiwen.ListShowActivity"
        android:label="ListShowActivity" />
    <fragment
        android:id="@+id/blankFragment"
        android:name="com.tin.projectlist.app.model.gushiwen.BlankFragment"
        android:label="fragment_blank"
        tools:layout="@layout/fragment_blank" >
        <action
            android:id="@+id/action_blankFragment_to_mainActivity"
            app:destination="@id/mainActivity" />
    </fragment>
</navigation>